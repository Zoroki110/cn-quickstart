# Development Environment for ClearportX Local
# Use Java Backend (port 8080) instead of Canton HTTP JSON API
REACT_APP_BACKEND_API_URL=http://localhost:4080
REACT_APP_CANTON_API_URL=http://localhost:2975
REACT_APP_NAME=ClearportX (Local Dev)

# Keycloak OAuth2 Configuration (via SSH tunnel on ports 4xxx)
REACT_APP_KEYCLOAK_URL=http://localhost:4082
REACT_APP_KEYCLOAK_REALM=AppProvider
REACT_APP_KEYCLOAK_CLIENT_ID=app-provider-unsafe

NODE_ENV=development
FAST_REFRESH=true
PORT=3001
